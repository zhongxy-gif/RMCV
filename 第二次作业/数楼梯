#include <iostream>
using namespace std;
int countWays(int n) {
    if (n == 1) return 1;
    if (n == 2) return 2;
    int prev2= 1;  
    int prev1= 2;  
    int current;
    for (int i = 3; i <= n; ++i) {
        current = prev2 + prev1; 
        prev2 = prev1;
        prev1 = current;
    }
    return prev1;
}

int main() {
    int n;
    cin >> n;
    cout << countWays(n) << endl;
    
    return 0;
}
